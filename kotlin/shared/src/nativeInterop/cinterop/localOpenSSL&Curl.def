compilerOpts = -ID:/vcpkg/packages/openssl_x64-windows/include \
                -ID:/vcpkg/packages/curl_x64-windows/include \
                -IC:/vcpkg/installed/x64-windows/include \
                -I/usr/include \
                -I/usr/local/ssl/include \
                -I/usr/include/x86_64-linux-gnu/

linkerOpts  = -LD:/vcpkg/packages/openssl_x64-windows/lib \
                -LD:/vcpkg/packages/curl_x64-windows/lib \
                -LC:/vcpkg/installed/x64-windows/lib \
                -L/usr/local/ssl/lib